{
  "name": "winston-datadog-browser",
  "version": "0.0.0-development",
  "description": "Send winston logs to Datadog via the browser logs SDK.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "keywords": [
    "browser",
    "datadog",
    "error",
    "log",
    "logger",
    "logging",
    "logs",
    "plugin",
    "typescript",
    "winston"
  ],
  "scripts": {
    "prelint": "prettier . -c",
    "lint": "eslint src",
    "prelint:fix": "prettier . -w",
    "lint:fix": "npm run lint -- --fix",
    "build": "tsc",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prepublish": "npm run build",
    "prepare": "husky",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manbearwiz/winston-datadog-browser.git"
  },
  "author": "Kevin Brey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/manbearwiz/winston-datadog-browser/issues"
  },
  "homepage": "https://github.com/manbearwiz/winston-datadog-browser#readme",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@tsconfig/recommended": "^1.0.3",
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^20.12.12",
    "@typescript-eslint/eslint-plugin": "^8.12.0",
    "@typescript-eslint/parser": "^6.4.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.27.5",
    "happy-dom": "^15.7.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.2",
    "semantic-release": "^24.0.0",
    "typescript": "^5.2.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "@datadog/browser-logs": "^4.0.0 || ^5.0.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "vitest related --run"
    ],
    "**/*": "prettier --write --ignore-unknown"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "overrides": {
    "tsconfig-paths": "^4.0.0"
  }
}
